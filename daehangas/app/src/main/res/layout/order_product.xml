<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="35dp">

        <TextView
            android:id="@+id/tv_productNm"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="5"
            android:text="DHCO200273"
            android:paddingLeft="12dp"
            android:textColor="#000000"
            android:gravity="center_vertical"
            android:textSize="@dimen/button_4_size" />
        <TextView
            android:id="@+id/tv_productId"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:visibility="invisible"
            android:text="1"/>
        <TextView
            android:id="@+id/tv_productCapa"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="4"
            android:textSize="@dimen/button_4_size"
            android:textColor="#000000"
            android:gravity="center_vertical"
            android:text="CO2(의료용)" />
        <TextView
            android:id="@+id/tv_productPriceSeq"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:visibility="invisible"
            android:text="1"/>
        <TextView
            android:id="@+id/tv_orderCount"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:textSize="@dimen/button_4_size"
            android:textColor="#000000"
            android:layout_gravity="left"
            android:gravity="center_vertical"
            android:text="20Kg" />

        <TextView
            android:id="@+id/tv_orderProductEtc"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="2"
            android:textSize="@dimen/button_long_size"
            android:textColor="#000000"
            android:layout_gravity="left"
            android:gravity="center_vertical"
            />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="35dp">

        <TextView
            android:id="@+id/tv_bottleChangeYnText"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="교환()"
            android:paddingLeft="10dp"
            android:textColor="#000000"
            android:gravity="center_vertical"
            android:layout_gravity="left"
            android:textSize="@dimen/button_4_size" />
        <TextView
            android:id="@+id/tv_bottleChangeYn"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:visibility="invisible"
            android:text="Y"/>
        <TextView
            android:id="@+id/tv_bottleSaleYnText"
            android:layout_width="@dimen/item_width2"
            android:layout_height="match_parent"
            android:textSize="@dimen/button_4_size"
            android:textColor="#000000"
            android:gravity="center_vertical"
            android:layout_gravity="left"
            android:text="판매()" />
        <TextView
            android:id="@+id/tv_bottleSaleYn"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:visibility="invisible"
            android:text="N"/>
        <TextView
            android:id="@+id/tv_retrievedYnText"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:textSize="@dimen/button_4_size"
            android:textColor="#000000"
            android:gravity="center_vertical"
            android:layout_gravity="left"
            android:text="회수()" />
        <TextView
            android:id="@+id/tv_retrievedYn"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:visibility="invisible"
            android:text="N"/>
        <TextView
            android:id="@+id/tv_asYnText"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:textSize="@dimen/button_4_size"
            android:textColor="#000000"
            android:gravity="center_vertical"
            android:layout_gravity="left"
            android:text="" />
        <TextView
            android:id="@+id/tv_asYn"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:visibility="invisible"
            android:text="N"/>
        <TextView
            android:id="@+id/tv_incomeYnText"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:textSize="@dimen/button_4_size"
            android:textColor="#000000"
            android:gravity="center_vertical"
            android:layout_gravity="left"
            android:text="" />
        <TextView
            android:id="@+id/tv_incomeYn"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:visibility="invisible"
            android:text="N"/>
        <TextView
            android:id="@+id/tv_outYnText"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:textSize="@dimen/button_4_size"
            android:textColor="#000000"
            android:gravity="center_vertical"
            android:layout_gravity="left"
            android:text="" />
        <TextView
            android:id="@+id/tv_outYn"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:visibility="invisible"
            android:text="N"/>
        <TextView
            android:id="@+id/tv_price"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:visibility="invisible"/>
        <Button
            android:id="@+id/btn_prdouctdel"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:background="@drawable/baseline_delete_forever_black_20" />
    </LinearLayout>
    <View style="@style/Divider" />
</LinearLayout>